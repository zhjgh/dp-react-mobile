{
  "name": "dp-react-mobile",
  "version": "0.1.0",
  "description": "react移动端组件",
  "keywords": [
    "component",
    "components",
    "design",
    "framework",
    "frontend",
    "react",
    "react-component",
    "ui",
    "Slider",
    "Ellipsis",
    "zhanghj"
  ],
  "homepage": "https://github.com/zhjgh/dp-react-mobile",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zhjgh/dp-react-mobile/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhjgh/dp-react-mobile.git"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "father:dev": "npx --max_old_space_size=8096 father doc dev --host 0.0.0.0",
    "father:build": "father build",
    "father:build:doc": "father doc build",
    "father:deploy": "father doc deploy"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "zhanghj",
  "module": "dist/dp-react-mobile.esm.js",
  "size-limit": [
    {
      "path": "dist/dp-react-mobile.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/dp-react-mobile.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "@babel/runtime": "7.7.1",
    "@types/swiper": "^5.4.1",
    "autoprefixer": "8.0.0",
    "father": "^2.30.2",
    "swiper": "4.5.1"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.9.2",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "cssnano": "4.1.10",
    "husky": "^4.3.8",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rollup-plugin-postcss": "4.0.0",
    "size-limit": "^4.9.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3"
  },
  "browserslist": [
    "last 2 versions",
    "Android >= 4.4",
    "iOS >= 9"
  ]
}
